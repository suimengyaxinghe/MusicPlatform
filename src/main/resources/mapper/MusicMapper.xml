<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.guoqiang.mapper.MusicMapper">
    <insert id="addMusic">
        insert into music (`musicname`,`singername`,`lyricist`,`composer`,`publishtime`,`tongue`,`musictime`) values (#{musicname},#{singername},#{lyricist},#{composer},#{publishtime},#{tongue},#{musictime})
    </insert>

    <select id="findALL" resultType="com.guoqiang.entity.Music">
        select musicname,singername,lyricist,composer,tongue,publishtime,musictime from music
    </select>
    <select id="findByMusicname" resultType="com.guoqiang.entity.Music">
        select * from music where musicname = #{musicname}
    </select>
    <select id="findByTongue" resultType="com.guoqiang.entity.Music">
        select * from music where tongue = #{tongue}
    </select>
</mapper>